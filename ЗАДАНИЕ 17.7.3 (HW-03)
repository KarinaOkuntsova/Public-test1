per_cent = {'ТКБ': 5.6, 'СКБ': 5.9, 'ВТБ': 4.28, 'СБЕР': 4.0}

int_money = int(input("Введите сумму: "))

deposit = (int_money*per_cent['ТКБ'],int_money*per_cent['СКБ'],int_money*per_cent['ВТБ'],int_money*per_cent['СБЕР'])

deposit2 = (list(map(round, deposit)))

print(deposit2)

deposit3 = max(list(deposit2))

print('Максимальная сумма, которую вы можете заработать: ', deposit3)
